{
  "name": "drupal",
  "env_run_lists": {},
  "default_attributes": {
    "mysql": {
      "server_root_password": "root",
      "server_repl_password": "root",
      "server_debian_password": "root"
    },
    "vm": {
    "ip": "192.168.44.44",
    "memory": "1024",
    "synced_folders": [
      {
        "host_path": "data/",
        "guest_path": "/var/www",
        "type": "default"
      }
    ],
    "forwarded_ports": []
    },
    "vdd": {
    "sites": {
      "drupal8": {
        "account_name": "root",
        "account_pass": "root",
        "account_mail": "box@example.com",
        "site_name": "Drupal 8",
        "site_mail": "box@example.com",
        "vhost": {
          "document_root": "drupal8",
          "url": "drupal8.dev",
          "alias": ["www.drupal8.dev"]
        }
      },
      "drupal7": {
        "account_name": "root",
        "account_pass": "root",
        "account_mail": "box@example.com",
        "site_name": "Drupal 7",
        "site_mail": "box@example.com",
        "vhost": {
          "document_root": "drupal7",
          "url": "drupal7.dev",
          "alias": ["www.drupal7.dev"]
        }
      }
    }
    }
  },
  "override_attributes": {},
  "json_class": "Chef::Role",
  "run_list": [
    "recipe[apt]",
    "recipe[apache2]",
    "recipe[mysql::client]",
    "recipe[mysql::server]",
    "recipe[drupal::apache]",
    "recipe[vdd::graphviz]",
    "recipe[drupal::vdd_sites]",
    "recipe[vdd::php]",
    "recipe[vdd::uploadprogress]",
    "recipe[vdd::mailcatcher]",
    "recipe[vdd::phpmyadmin]",
    "recipe[vdd::mysql]",
    "recipe[vdd::xdebug]",
    "recipe[drupal::git]",
    "recipe[vdd::webgrind]",
    "recipe[vdd::mc]",
    "recipe[vdd::apc]",
    "recipe[vdd::rsync]",
    "recipe[vdd::curl]",
    "recipe[vdd::composer]",
    "recipe[vdd::drush]",
    "recipe[drupal::vdd_help]",
    "recipe[vdd::vim]",
    "recipe[vdd::patchutils]",
    "recipe[vdd::siege]",
    "recipe[vdd_example]"
  ],
  "description": "Installs Drupal development environment.",
  "chef_type": "role"
}
